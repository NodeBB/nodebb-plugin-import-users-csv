{
  "name": "@nodebb/nodebb-plugin-import-users-csv",
  "version": "1.2.1",
  "description": "A starter kit for quickly creating NodeBB plugins",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodebb/nodebb-plugin-import-users-csv"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "import-users-csv",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodebb/nodebb-plugin-import-users-csv/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^3.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-angular": "19.3.0",
    "eslint": "9.9.0",
    "eslint-config-nodebb": "0.2.1",
    "eslint-plugin-import": "2.29.1",
    "husky": "9.1.4",
    "lint-staged": "15.2.9"
  },
  "dependencies": {
    "csv-parse": "^5.4.0"
  }
}
